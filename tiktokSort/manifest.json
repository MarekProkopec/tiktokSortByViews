{
    "name": "Tiktok Sort",
    "description": "Sorts tiktok videos of a creator by number of views",
    "version": "1.1",
    "manifest_version": 3,

    "content_scripts": [{
        "js": ["sortByViews.js"],
        "matches": ["https://www.tiktok.com/*"]
    }],

    "default_icon": {
        "16": "/logos/logo16.png",
        "32": "/logos/logo32.png",
        "48": "/logos/logo48.png",
        "128": "/logos/logo128.png"
    },
    "icons": {
        "16": "/logos/logo16.png",
        "32": "/logos/logo32.png",
        "48": "/logos/logo48.png",
        "128": "/logos/logo128.png"
    }
}